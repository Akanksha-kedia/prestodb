             <scope>compile</scope>
         </dependency>
 
@@ -432,7 +432,7 @@
         <dependency>
             <groupId>com.fasterxml.jackson.core</groupId>
             <artifactId>jackson-annotations</artifactId>
-            <version>2.10.2</version>
+            <version>2.15.3</version>
             <scope>provided</scope>
         </dependency>
 
@@ -540,7 +540,7 @@
                 </plugin>
